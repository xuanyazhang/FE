{
    "title" : "前端学习",
    "links" : { "sidebar" : { "首页" : "http://zhangxy.github.io" } },
    "theme-default": {
        "showLevel": true
    },
    "plugins": ["github","theme-comscore","-search", "github-buttons", "links", "edit-link"],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/xuanyazhang/FE"
        },
        "links": {
            "gitbook": false,
            "sharing": {
                "google": true,
                "facebook": true,
                "twitter": false,
