            }, {
                "user" : "xuanyazhang",
                "repo" : "FE",
                "type" : "fork",
                "size" : "small",
                "count": true
            }]
        },
        "edit-link": {
            "base": "https://github.com/xuanyazhang/FE/blob/master/",
            "label": "Edit This Page"
        }
